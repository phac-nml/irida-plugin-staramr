    </parameter>
    <parameter name="staramr-2-advanced.pid_threshold" defaultValue="98.0">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/staramr/staramr_search/0.7.1" parameterName="advanced.pid_threshold"/>
