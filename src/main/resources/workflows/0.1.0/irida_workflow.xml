    </parameter>
    <parameter name="staramr-2-advanced.report_all_blast" defaultValue="false">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/staramr/staramr_search/0.7.1" parameterName="advanced.report_all_blast"/>
